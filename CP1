    "def make_3d_plot(g1: Good, g2: Good, budget, func_u_from_x1):\n",
    "    def local_3d(x1, x2, budget):\n",
    "        x_val = np.linspace(0, budget / x1, 100)\n",
    "        y_val = np.linspace(0, budget / x2, 100)\n",
    "        x, y = np.meshgrid(x_val, y_val)\n",
    "        u = func_u_from_x1(x, y)\n",
    "\n",
    "        # Рисуем сам график\n",
    "        fig = plt.figure()\n",
    "        ax = fig.add_subplot(111, projection='3d')\n",
    "        ax.plot_surface(x, y, u, cmap='viridis')\n",
    "\n",
    "        # Даём ему название\n",
    "        plt.title(\"U($x_1, x_2$)\")\n",
    "\n",
    "        # Подписываем оси\n",
    "        plt.xlabel('$x_1$')\n",
    "        plt.ylabel('$x_2$')\n",
    "\n",
    "        # Добавляем координатную сетку\n",
    "        plt.grid()\n",
    "        plt.show()\n",
    "    interact(local_3d, x1 = (1, 20000), x2 = (1, 20000), budget = (1, 40000))\n",
    "\n",
    "make_3d_plot(Good(\"x1\", 5000), Good(\"x2\", 10000), 20000, x05_u_from_x1_x2)"
   ]
