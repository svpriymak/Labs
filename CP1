    "assert budget_test_several_restrictions([(Good(\"Корм\", 10), Good(\"Вкусняшки\", 5)), (Good(\"Корм\", 5), Good(\"Вкусняшки\", 10)), (Good(\"Корм\", 5), Good(\"Вкусняшки\", 5))], [100, 100, 150], 20,20) == -1\n",
    "assert budget_test_several_restrictions([(Good(\"Корм\", 10), Good(\"Вкусняшки\", 5)), (Good(\"Корм\", 5), Good(\"Вкусняшки\", 10)), (Good(\"Корм\", 5), Good(\"Вкусняшки\", 5))], [100, 100, 150], 30,0) == 0\n",
